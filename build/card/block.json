{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "rpmdelivery/card",
  "version": "0.1.0",
  "title": "Card",
  "category": "theme",
  "icon": "smiley",
  "description": "Block of a card with image, title and text.",
  "attributes": {
    "media": {
      "type": "object",
      "default": {
        "url": "",
        "id": 0
      }
    },
    "title": {
      "type": "string"
    }
  },
  "supports": {
    "html": false
  },
  "textdomain": "card",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}